<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>Topaidi</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>Accueil</servlet-name>
		<servlet-class>fr.epsi.servlet.AccueilServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Accueil</servlet-name>
		<url-pattern>/accueil</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Administration</servlet-name>
		<servlet-class>fr.epsi.servlet.AdministrationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Administration</servlet-name>
		<url-pattern>/administration</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>Classement</servlet-name>
		<servlet-class>fr.epsi.servlet.ClassementServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Classement</servlet-name>
		<url-pattern>/classement</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>CreationIdee</servlet-name>
		<servlet-class>fr.epsi.servlet.CreationIdeeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreationIdee</servlet-name>
		<url-pattern>/creationIdee</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>CreationNoteIdee</servlet-name>
		<servlet-class>fr.epsi.servlet.CreationNoteIdeeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreationNoteIdee</servlet-name>
		<url-pattern>/creationNoteIdee</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Inscription</servlet-name>
		<servlet-class>fr.epsi.servlet.InscriptionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Inscription</servlet-name>
		<url-pattern>/inscription</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ListIdee</servlet-name>
		<servlet-class>fr.epsi.servlet.ListIdeeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListIdee</servlet-name>
		<url-pattern>/list-idee</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>NoteIdee</servlet-name>
		<servlet-class>fr.epsi.servlet.NoteIdeeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NoteIdee</servlet-name>
		<url-pattern>/noteIdee</url-pattern>
	</servlet-mapping>









</web-app>